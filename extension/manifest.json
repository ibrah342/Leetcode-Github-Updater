{
    "manifest_version": 3,
    "name": "LeetCode GitHub Updater",
    "version": "1.0",
    "description": "Update GitHub with solved LeetCode problems.",
    "permissions": ["storage", "scripting"],
    "host_permissions": [
      "https://leetcode.com/*",
      "https://api.github.com/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  