{
    "manifest_version": 3,
    "name": "LeetCode GitHub Updater",
    "version": "1.0",
    "description": "Click to update solved LeetCode problems on GitHub",
    "permissions": ["activeTab", "storage", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "https://leetcode.com/*",
      "https://api.github.com/*"
    ]
  }
  